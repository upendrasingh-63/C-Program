#include <stdio.h>

int findMaximum(int a, int b) {
    return (a > b) ? a : b;
}

int findMinimum(int a, int b) {
    return (a < b) ? a : b;
}

int main() {
    int num1, num2;
    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);
    int max = findMaximum(num1, num2);
    int min = findMinimum(num1, num2);
    printf("Maximum: %d\n", max);
    printf("Minimum: %d\n", min);
    return 0;
}
